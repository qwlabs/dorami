{
    "name": "@dorami/lang",
    "version": "0.0.0",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "publishConfig": {
        "main": "./index.mjs",
        "module": "./index.mjs",
        "types": "./index.d.mts",
        "exports": {
            ".": {
                "types": "./index.d.mts",
                "import": "./index.mjs"
            }
        },
        "directory": "dist",
        "linkDirectory": false,
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "build": "NODE_ENV=production INPUT_DIR=./ OUTPUT_DIR=dist/ bun run build:package",
        "build:package": "bun run build:prebuild && tsup && bun run build:postbuild",
        "build:prebuild": "bun ./scripts/prebuild.ts",
        "build:postbuild": "bun ./scripts/postbuild.ts",
        "dev:link": "bun link --global && bun link"
    },
    "devDependencies": {
        "tsup": "^8.2.4"
    },
    "author": "Qwlabs",
    "homepage": "https://github.com/qwlabs/dorami",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/qwlabs/dorami.git"
    },
    "bugs": {
        "url": "https://github.com/qwlabs/dorami/issues"
    },
    "engines": {
        "node": ">=20.17.0",
        "bun": ">=1"
    }
}
